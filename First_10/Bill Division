public static void bonAppetit(List<Integer> bill, int k, int b) {
    // Write your code here
        bill.set(k,0);
        int sum = 0;
        
        for(int i:bill){
            sum += i;
        }
        
        sum = sum / 2;  //(sum - bill[k])/2
        
        if(sum == b){
            System.out.println("Bon Appetit");
        }
        else{
            System.out.println(b - sum);
        }
    }
